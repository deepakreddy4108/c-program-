#include<stdio.h>
struct student
{
	char name[30];
	int marks[5];
}
	int main()
{
	struct student s;
	printf("enter name:");
	scanf("%s",s.name);
	
	printf("enter marks for 5 subjects:");
	for(int i=0;i<5,i++)
{
    scanf("%d", &s.marks[i]);
    }

    printf("Name: %s\n", s.name);
    printf("Marks: ");
    for (int i = 0; i < 5; i++) 
	{
    printf("%d ", s.marks[i]);
    }
    
    return 0;
}
